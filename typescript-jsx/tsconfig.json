{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "jsx": "react",
    "jsxFactory": "elements.createElement",
    "lib": ["ESNext"],
    "allowJs": true,
    "checkJs": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
